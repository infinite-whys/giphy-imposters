{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "program": "${file}",
            "sourceMaps": true,
            "request": "launch",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                "LOG_LEVEL":"DEBUG"
            }
        },
        {
            "name": "Launch ",
            "program": "${workspaceFolder}/build/launch.js",
            "sourceMaps": true,
            "request": "launch",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                "LOG_LEVEL":"DEBUG"
            }
        },
    ]
}